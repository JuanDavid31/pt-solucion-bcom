<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog-ext
   http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd
   http://www.liquibase.org/xml/ns/dbchangelog
   http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">

    <changeSet author="Juan David" id="1">
        <createTable tableName="USUARIO">
            <column autoIncrement="true" name="ID" type="INT(10)">
                <constraints primaryKey="true" primaryKeyName="USUARIO_PK"/>
            </column>
            <column name="NOMBRE" type="VARCHAR2(30)"/>
            <column defaultValueComputed="NOW()" name="FECHA_CREACION" type="date">
                <constraints nullable="false"/>
            </column>
            <column defaultValueComputed="NOW()" name="FECHA_MODIFICACION" type="INT(10)">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>
</databaseChangeLog>